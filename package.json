{
  "name": "emporium",
  "version": "1.9.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve  --stats-json --proxy-config proxy.conf.json",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:client-and-server-bundles": "ng build --prod --stats-json && ng run emporium:server:production",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "compile:server": "tsc -p server.tsconfig.json",
    "serve:ssr": "node dist/server",
    "compile_@agm_core": "babel node_modules/@agm/core -d node_modules/@agm/core --presets @babel/preset-env",
    "postinstall": "npm run compile_@agm_core"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "1.0.0-beta.6",
    "@angular/animations": "8.2.1",
    "@angular/cdk": "8.1.2",
    "@angular/common": "8.2.1",
    "@angular/compiler": "8.2.1",
    "@angular/core": "11.0.5",
    "@angular/flex-layout": "8.0.0-beta.26",
    "@angular/forms": "8.2.1",
    "@angular/material": "8.1.2",
    "@angular/platform-browser": "8.2.1",
    "@angular/platform-browser-dynamic": "8.2.1",
    "@angular/platform-server": "8.2.1",
    "@angular/router": "8.2.1",
    "@angular/service-worker": "^9.0.2",
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@nguniversal/express-engine": "8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "@ngx-lite/json-ld": "^0.6.2",
    "cors": "^2.8.5",
    "express": "4.17.1",
    "hammerjs": "2.0.8",
    "ngx-cookie-service": "^2.3.0",
    "ngx-pagination": "4.1.0",
    "ngx-perfect-scrollbar": "8.0.0",
    "ngx-spinner": "8.0.3",
    "ngx-swiper-wrapper": "8.0.2",
    "rxjs": "6.5.2",
    "ts-loader": "6.0.4",
    "zone.js": "0.10.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^9.1.0",
    "@angular-devkit/build-angular": "0.802.1",
    "@angular/cli": "8.2.1",
    "@angular/compiler-cli": "8.2.1",
    "@angular/language-service": "8.2.1",
    "@babel/cli": "7.5.5",
    "@babel/core": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@types/jasmine": "3.4.0",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "12.7.1",
    "codelyzer": "5.1.0",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.2.0",
    "karma-chrome-launcher": "3.0.0",
    "karma-coverage-istanbul-reporter": "2.1.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "protractor": "5.4.2",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "typescript": "3.5.3",
    "webpack-bundle-analyzer": "^3.7.0"
  }
